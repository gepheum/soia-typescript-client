{
  "name": "skir-client",
  "version": "0.0.5",
  "description": "",
  "homepage": "https://github.com/gepheum/skir-typescript-client#readme",
  "bugs": {
    "url": "https://github.com/gepheum/skir-typescript-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gepheum/skir-typescript-client.git"
  },
  "license": "ISC",
  "author": "Tyler Fibonacci <gepheum@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/skir-client.js",
      "require": "./dist/cjs/skir-client.js"
    }
  },
  "main": "./dist/cjs/skir-client.js",
  "module": "./dist/esm/skir-client.js",
  "files": [
    "dist/cjs",
    "dist/esm",
    "src",
    "!**/*.test.ts"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p ./tsconfig.cjs.json && cp static/package-cjs.json dist/cjs/package.json",
    "build:esm": "tsc -p ./tsconfig.esm.json && cp static/package-esm.json dist/esm/package.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "mocha",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "buckwheat": "^1.1.2",
    "eslint": "^8.54.0",
    "express": "^5.1.0",
    "mocha": "^10.2.0",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^4.2.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/express": "^5.0.1"
  }
}
